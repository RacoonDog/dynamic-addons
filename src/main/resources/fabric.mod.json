{
  "schemaVersion": 1,
  "id": "meteor-dynamic-addons",
  "version": "${version}",
  "name": "Meteor Dynamic Addons",
  "description": "Enables Disabling/Reloading addons at runtime.",
  "authors": [
    "Crosby"
  ],
  "contact": {
    "repo": "https://github.com/RacoonDog/meteor-addon-template",
    "sources": "https://github.com/RacoonDog/meteor-addon-template",
    "issues": "https://github.com/RacoonDog/meteor-addon-template/issues"
  },
  "license": "MPL-2.0",
  "icon": "assets/meteor-dynamic-addons/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "io.github.racoondog.dynamicaddons.DynamicAddons"
    ]
  },
  "mixins": [
    "meteor-dynamic-addons.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25",
    "modmenu": {
      "badges": [ "meteor" ]
    }
  },
  "depends": {
    "java": ">=17",
    "fabricloader": ">=0.12",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
